name = input("Enter file:")
if len(name) < 1:
    name = "mbox-short.txt"
fh = open(name)
emails = {}

for line in fh: 
    line = line.rstrip() 
    if not line.startswith('From '): 
        continue
    else:
    	words = line.split()[1]
        emails[words] = emails.get(words,0) + 1

largest = -1
email = None

for k,v in emails.items(): 
    if v > largest: 
        largest = v
        email = k 
        
print(email, largest)
